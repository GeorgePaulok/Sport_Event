<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <servlet>
        <servlet-name>Servlet</servlet-name>
        <servlet-class>com.apache.Servlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>loginServlet</servlet-name>
        <servlet-class>com.apache.loginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Group_Add_Servlet</servlet-name>
        <servlet-class>com.apache.Group_Add_Servlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Event_Add_Servlet</servlet-name>
        <servlet-class>com.apache.Event_Add_Servlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Sports_Event_Add_Servlet</servlet-name>
        <servlet-class>com.apache.Sports_Event_Add_Servlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Team_Login_Servlet</servlet-name>
        <servlet-class>com.apache.Team_Login_Servlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Team_Register_Servlet</servlet-name>
        <servlet-class>com.apache.Team_Register_Servlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Delete_Player_Servlet</servlet-name>
        <servlet-class>com.apache.Delete_Player_Servlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SignUp_Servlet</servlet-name>
        <servlet-class>com.apache.SignUp_Servlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Servlet</servlet-name>
        <url-pattern>/Servlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>loginServlet</servlet-name>
        <url-pattern>/loginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Group_Add_Servlet</servlet-name>
        <url-pattern>/Group_Add_Servlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Event_Add_Servlet</servlet-name>
        <url-pattern>/Event_Add_Servlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Sports_Event_Add_Servlet</servlet-name>
        <url-pattern>/Sports_Event_Add_Servlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Team_Register_Servlet</servlet-name>
        <url-pattern>/Team_Register_Servlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Team_Login_Servlet</servlet-name>
        <url-pattern>/Team_Login_Servlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Delete_Player_Servlet</servlet-name>
        <url-pattern>/Delete_Player_Servlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SignUp_Servlet</servlet-name>
        <url-pattern>/SignUp_Servlet</url-pattern>
    </servlet-mapping>
</web-app>